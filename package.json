{
  "name": "lightlist-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@11.6.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "format": "turbo run format",
    "typecheck": "turbo run typecheck",
    "deploy:firestore": "cd packages/sdk && firebase deploy --only firestore:rules,firestore:indexes",
    "deploy:firestore:prod": "cd packages/sdk && firebase deploy --only firestore:rules,firestore:indexes --project prod"
  },
  "devDependencies": {
    "firebase-tools": "^15.5.1",
    "prettier": "^3.6.2",
    "turbo": "^2.0.0"
  },
  "overrides": {
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native-screens": "4.16.0"
  }
}
